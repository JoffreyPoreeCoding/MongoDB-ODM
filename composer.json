{
    "name": "jpc/mongodb-odm",
    "description": "A small MongoDB ODM",
    "type": "library",
    "keywords": [
        "MongoDB",
        "ODM",
        "PHP",
        "Mapping",
        "Database",
        "ORM"
    ],
    "homepage": "https://github.com/JoffreyPoreeCoding/MongoDB-ODM",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "mongodb/mongodb": "^1.9"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.3",
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.2"
    },
    "authors": [
        {
            "name": "Joffrey Poree",
            "email": "poree.joffrey@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "JPC\\MongoDB\\ODM\\": "src/"
        },
        "psr-0": {
            "JPC\\MongoDB\\ODM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JPC\\Test\\MongoDB\\ODM\\": "tests/"
        },
        "psr-0": {
            "JPC\\Test\\MongoDB\\ODM\\": "tests/"
        }
    }
}
