{
    "name": "jpc/mongodb-odm",
    "description": "A small MongoDB ODM",
    "type": "library",
    "keywords": [
        "MongoDB",
        "ODM",
        "PHP",
        "Mapping",
        "Database",
        "ORM"
    ],
    "homepage": "https://github.com/JoffreyPoreeCoding/MongoDB-ODM",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "mongodb/mongodb": "^1.9",
        "symfony/cache": "^5.3|^6.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.3|^6.0",
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.2",
        "codedungeon/phpunit-result-printer": "^0.32.0"
    },
    "authors": [
        {
            "name": "Joffrey Poree",
            "email": "poree.joffrey@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "JPC\\MongoDB\\ODM\\": "src/"
        },
        "psr-0": {
            "JPC\\MongoDB\\ODM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JPC\\MongoDB\\ODM\\Tests\\": "tests/"
        },
        "psr-0": {
            "JPC\\MongoDB\\ODM\\Tests\\": "tests/"
        }
    }
}